{
  "name": "kungfu-cli",
  "version": "2.2.0",
  "description": "KungFu Trader Cli",
  "license": "Apache-2.0",
  "homepage": "https://github.com/taurusai/kungfu",
  "author": {
    "name": "taurus.ai",
    "email": "info@taurus.ai"
  },
  "config": {
    "pyver": "three",
    "cmakejsopt": ""
  },
  "main": "./dist/cli/index.js",
  "engines": {
    "node": ">=10.16.0 <11.0.0"
  },
  "scripts": {
    "install-deps": "cd ./src/assets/static/js/inquirer-path && yarn install && yarn compile",
    "clean": "rimraf ./dist/* ./build/*",
    "build": "node -v",
    "build-cli": "node ./.build/build.js && yarn run cp",
    "build-linux": " yarn run dist && pkg . -t=node10.17-linux-x64 --out-path=build/cli",
    "build-mac": " yarn run dist && pkg . -t=node10.17-macos-x64 --out-path=build/cli",
    "dev": "node .build/dev-cli.js",
    "cli": "node dist/cli/index",
    "dist": "cross-env NODE_ENV=production node --max_old_space_size=8192 ../node_modules/webpack/bin/webpack.js --progress --colors --config .build/webpack.cli.config.js",
    "test": " mocha",
    "cp": "yarn run cp-node",
    "cp-node": "copyfiles -a -f ../core/build/*/libkungfu.* ../core/build/*/kungfu.* ../core/build/*/pykungfu* ../core/build/*/node_kungfu.node ../core/build/*/kungfubuildinfo.json ./build/cli"
  },
  "bin": "./dist/cli/index.js",
  "pkg": {
    "scripts": [
      "../node_modules/bodec/bodec-node.js"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "bindings": "^1.5.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.16",
    "cfonts": "2.4.5",
    "colors": "^1.3.3",
    "commander": "^2.20.0",
    "encoding": "^0.1.12",
    "fast-csv": "^2.5.0",
    "fkill": "^6.1.0",
    "fs-extra": "7.0.1",
    "gaze": "^1.1.3",
    "getprocesses": "^1.1.1",
    "inquirer": "^6.3.1",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "kungfu-core": "*",
    "log4js": "^4.0.2",
    "moment": "^2.24.0",
    "node-schedule": "^1.3.2",
    "pm2": "4.4.0",
    "readline": "^1.3.0",
    "tail": "^2.0.2",
    "taskkill": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.6.2",
    "aws-sdk": "^2.395.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "builder-util": "^21.2.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-roughly": "^1.0.0",
    "chalk": "^2.1.0",
    "codemirror": "^5.44.0",
    "colors": "^1.3.3",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.0.5",
    "cross-var": "^1.1.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow": "^0.2.3",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "jquery": "^3.4.1",
    "mime": "^2.4.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "moment": "^2.24.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "optimize-js-plugin": "^0.0.4",
    "pkg": "^4.4.0",
    "preload-webpack-plugin": "^2.3.0",
    "readline": "^1.3.0",
    "require-dir": "^1.0.0",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "ts-loader": "3.5.0",
    "typescript": "^3.5.3",
    "webpack": "^3.5.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-server": "3.0.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "webpack-permissions-plugin": "^1.0.0"
  }
}
