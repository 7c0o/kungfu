{
  "name": "kfext_task_ticker-filter",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "install-deps": "node -v",
    "dist": "cross-env NODE_ENV=production node --max_old_space_size=2048 ../node_modules/webpack/bin/webpack.js --progress --colors --config .build/webpack.ext.config.js",
    "build": "yarn run dist && yarn run cp",
    "build-cli": "yarn run dist && yarn run cp",
    "cp": "copyfiles -a -f ./package.json ./lib",
    "clean": "rimraf ./lib/*",
    "dev": "node .build/dev-ext.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "kungfu-shared": "*",
    "minimist": "^1.2.5",
    "moment": "^2.29.1",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.1",
    "@types/node": "^14.14.20",
    "copyfiles": "^2.4.1",
    "pkg": "^4.4.9",
    "rimraf": "^3.0.2"
  },
  "bin": "./lib/index.js",
  "kungfuConfig": {
    "type": "task",
    "key": "backwardsHedge",
    "name": "最大贴水",
    "uniKey": [
      "accountId",
      "tickerSet"
    ],
    "config": [
      {
        "key": "accountId",
        "name": "交易账户",
        "type": "account",
        "required": true
      },
    
      {
        "key": "tickerSet",
        "name": "标的池",
        "type": "tickerSet",
        "tickersKey": "tickerSetTickers",
        "required": true,
        "tip": "需要预先在行情模块中设置标的池"
      },
      {
        "key": "index",
        "name": "对应指数",
        "type": "instrumentIdInTickerSet",
        "required": true,
        "tip": "对应指数需包含在所选标的池内"
      },
      {
        "key": "side",
        "name": "买卖",
        "type": "radio",
        "data": [
          {
            "name": "买",
            "value": 0
          },
          {
            "name": "卖",
            "value": 1
          }
        ],
        "default": 0,
        "required": true
      },
      {
        "key": "offset",
        "name": "开平",
        "type": "radio",
        "options": "offset",
        "default": 0,
        "required": true
      },
      {
        "key": "volume",
        "name": "目标数量",
        "type": "int",
        "required": true
      },
      {
        "key": "triggerTime",
        "name": "触发时间",
        "type": "timePicker",
        "required": true
      },
      {
        "key": "finishTime",
        "name": "结束时间",
        "type": "timePicker",
        "laterThan": "triggerTime",
        "required": true
      },
      {
        "key": "interval",
        "name": "交易间隔",
        "type": "int",
        "tip": "单位毫秒ms",
        "required": true,
        "default": 5000
      },
      {
        "key": "maxBackward",
        "name": "最大年化贴水",
        "type": "int",
        "tip": "单位%",
        "required": true,
        "default": 12
      }
    ],

    "displayConfig": [
      {
        "type": "text",
        "label": "下单时间",
        "prop": "updateTime",
        "width": "70px"
      },{
        "type": "text",
        "label": "标的",
        "prop": "instrumentId",
        "width": "60px"
      },{
        "type": "text",
        "label": "对应指数",
        "prop": "indexId",
        "width": "60px"
      },{
        "type": "number",
        "label": "合约价格",
        "prop": "instrumentPrice",
        "width": "60px"
      },{
        "type": "number",
        "label": "期现差",
        "prop": "delta",
        "width": "60px"
      },{
        "type": "string",
        "label": "升贴水比例",
        "prop": "backWardsRatio",
        "width": "80px"
      },{
        "type": "string",
        "label": "升贴水年化",
        "prop": "backWardsRatioByYears",
        "width": "80px"
      },{
        "type": "string",
        "label": "交割日期",
        "prop": "expiredDate",
        "width": "80px"
      },{
        "type": "number",
        "label": "指数价格",
        "prop": "indexPrice",
        "width": "60px"
      },{
        "type": "text",
        "label": "",
        "prop": "side",
        "width": "40px"
      },{
        "type": "text",
        "label": "",
        "prop": "offset",
        "width": "40px"
      },{
        "type": "number",
        "label": "委托价",
        "prop": "limitPrice",
        "width": "80px"
      },{
        "type": "number",
        "label": "剩余时间(ms)",
        "prop": "remainTime",
        "width": "100px"
      },{
        "type": "text",
        "align": "center",
        "label": "下单量",
        "prop": "volume",
        "width": "60px"
      },{
        "type": "text",
        "align": "center",
        "label": "成交/目标量",
        "prop": "volumeTraded",
        "width": "90px"
      }
    ]
  }
}
