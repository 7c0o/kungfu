{
  "name": "kungfu-core",
  "author": "taurus.ai",
  "version": "2.3.0",
  "description": "KungFu Trader Core",
  "license": "Apache-2.0",
  "main": "node/index.js",
  "repository": {
    "url": "https://github.com/kungfu-trader/kungfu.git"
  },
  "config": {
    "buildtype": "Release",
    "loglevel": "trace",
    "arch": "x64",
    "pypi": "http://mirrors.aliyun.com/pypi/simple",
    "python_version": "3.7",
    "node_version": "10.16.0",
    "electron_version": "4.2.11",
    "runtime": "electron"
  },
  "binary": {
    "module_name": "node_kungfu",
    "module_path": "build/kfc",
    "remote_path": "core/v{version}",
    "package_name": "kfc-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://kungfu.s3.cn-northwest-1.amazonaws.com.cn"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "build": "node-pre-gyp configure build package",
    "build-cli": "node-pre-gyp configure build package",
    "pipenv-install": "cross-var pipenv --pypi-mirror $npm_package_config_pypi --python $npm_package_config_python_version install",
    "configure": "cross-var pipenv run conan install . -if build -s build_type=$npm_package_config_buildtype --build missing -o log_level=$npm_package_config_loglevel -o arch=$npm_package_config_arch -o js_runtime=$npm_package_config_runtime -o node_version=$npm_package_config_node_version -o electron_version=$npm_package_config_electron_version",
    "compile": "cross-var pipenv run conan build . -bf build -s build_type=$npm_package_config_buildtype",
    "freeze": "cross-var pipenv run conan package . -bf build -pf build/kfc -s build_type=$npm_package_config_buildtype",
    "clean": "cmake-js clean",
    "format": "pipenv run conan source .",
    "conan": "pipenv run conan",
    "dev": "pipenv run python python/dev_run.py",
    "test": "pipenv run python python/dev_test.py"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "fs-extra": "7.0.1",
    "glob": "^7.1.6",
    "node-pre-gyp": "^0.17.0",
    "nan": "^2.14.0",
    "node-addon-api": "==2.0.0"
  },
  "devDependencies": {
    "cmake-js": "==6.1.0",
    "cross-env": "^5.0.5",
    "cross-var": "^1.1.0"
  }
}
