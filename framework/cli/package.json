{
  "name": "@kungfu-trader/kungfu-cli",
  "author": {
    "name": "kungfu-trader",
    "email": "info@kungfu.link"
  },
  "version": "2.4.0-alpha.4",
  "description": "Kungfu CLI",
  "license": "Apache-2.0",
  "main": "./dist/cli/index.js",
  "repository": {
    "url": "https://github.com/kungfu-trader/kungfu.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "postinstall": "cd ./src/assets/static/js/inquirer-path && yarn install && yarn compile",
    "clean": "rimraf ./dist/* ./build/*",
    "build": "node ./.build/build.js && yarn run cp",
    "build-win": " yarn run dist && pkg . -t=node10-win-x64 --output=build/cli",
    "build-linux": " yarn run dist && pkg . -t=node10-linux-x64 --out-path=build/cli",
    "build-mac": " yarn run dist && pkg . -t=node10-macos-x64 --out-path=build/cli",
    "dev": "node .build/dev-cli.js",
    "cli": "node dist/cli/index",
    "dist": "cross-env NODE_ENV=production node --max_old_space_size=2048 ../../node_modules/webpack/bin/webpack.js --progress --colors --config .build/webpack.cli.config.js",
    "cp": "yarn run cp-node",
    "cp-node": "copyfiles -a -f ../core/build/*/libkungfu.* ../core/build/*/pykungfu* ../core/build/*/*.node ../core/build/*/kungfubuildinfo.json ./build/cli",
    "freeze": "pkg -t=node10-macos-x64 --debug --out-path=build/cli dist/cli/index.js"
  },
  "bin": "./dist/cli/index.js",
  "pkg": {
    "scripts": [
      "../node_modules/bodec/bodec-node.js"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@kungfu-trader/kungfu-core": "^2.4.0-alpha.4",
    "@kungfu-trader/kungfu-shared": "^2.4.0-alpha.4",
    "bindings": "^1.5.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.16",
    "cfonts": "2.4.5",
    "colors": "^1.3.3",
    "commander": "^2.20.0",
    "encoding": "^0.1.12",
    "fast-csv": "^2.5.0",
    "fkill": "^6.1.0",
    "fs-extra": "7.0.1",
    "gaze": "^1.1.3",
    "getprocesses": "^1.1.1",
    "glob": "^7.1.6",
    "inquirer": "^6.3.1",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "log4js": "^4.0.2",
    "moment": "^2.24.0",
    "node-schedule": "^1.3.2",
    "pm2": "3.5.2",
    "taskkill": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/node": "^12.6.2",
    "aws-sdk": "^2.395.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babili-webpack-plugin": "^0.1.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-roughly": "^1.0.0",
    "chalk": "^4.1.0",
    "colors": "^1.3.3",
    "compression-webpack-plugin": "^8.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^2.4.0",
    "cross-env": "^5.0.5",
    "cross-var": "^1.1.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow": "^0.2.3",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.4.1",
    "mime": "^2.4.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "optimize-js-plugin": "^0.0.4",
    "pkg": "^5.3.0",
    "require-dir": "^1.0.0",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "ts-loader": "3.5.0",
    "typescript": "^4.3.5",
    "webpack": "^3.5.2",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.8.0",
    "webpack-permissions-plugin": "^1.0.0"
  }
}
