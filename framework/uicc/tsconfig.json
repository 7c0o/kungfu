{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types",
      "../../node_modules/@types",
      "typings"
    ],
    "baseUrl": "src",
    "paths": {
      "@kungfu-trader/kungfu-uicc/*": [
        "*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}