  },
  "version": "2.4.0-alpha.4",
  "description": "Kungfu App",
  "license": "Apache-2.0",
  "main": "./dist/app/main.js",
  "repository": {
    "url": "https://github.com/kungfu-trader/kungfu.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "env": [
    "USE_HARD_LINKS=false"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "clean": "rimraf ./dist/* ./build/*",
    "dev": "node .electron-kungfu/dev-runner.js",
    "dist:daemon": "yarn webpack-cli --config .electron-kungfu/webpack.daemon.config.js --stats-error-details",
    "dist:main": "yarn webpack-cli --config .electron-kungfu/webpack.main.config.js --stats-error-details",
    "dist:render": "yarn webpack-cli --config .electron-kungfu/webpack.renderer.config.js --stats-error-details",
    "dist": "node --max_old_space_size=4096 .electron-kungfu/build.js",
    "elec": "node .electron-builder/build.js",
    "prebuild": "yarn run clean && yarn run dist",
    "build": "yarn run elec",
    "prebuild-cli": "yarn run clean && yarn run dist",
    "build-cli": "yarn run elec --cli",
    "webpack": "cross-env NODE_ENV=production node --max_old_space_size=2048 ../../node_modules/webpack/bin/webpack.js"
  },
  "dependencies": {
