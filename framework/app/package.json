{
  "name": "@kungfu-trader/kungfu-app",
  "author": {
    "name": "kungfu-trader",
    "email": "info@kungfu.link"
  },
  "version": "2.4.0-alpha.4",
  "description": "Kungfu App",
  "license": "Apache-2.0",
  "main": "dist/app/main.js",
  "repository": {
    "url": "https://github.com/kungfu-trader/kungfu.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "env": [
    "USE_HARD_LINKS=false"
  ],
  "scripts": {
    "clean": "rimraf ./dist/* ./build/*",
    "dev": "node .devtools/dev-runner.js",
    "dist:daemon": "yarn webpack-cli --config .webpack/webpack.daemon.config.js --stats-error-details",
    "dist:main": "yarn webpack-cli --config .webpack/webpack.main.config.js --stats-error-details",
    "dist:render": "yarn webpack-cli --config .webpack/webpack.renderer.config.js --stats-error-details",
    "dist": "node --max_old_space_size=4096 .webpack/build.js",
    "prebuild": "yarn run clean && yarn run dist",
    "build": "node .electron-builder/build.js",
    "rebuild": "rm -rf build && yarn build",
    "format": "prettier --write --parser typescript \"(.*)/**.js\""
  },
  "dependencies": {
    "@authing/vue-ui-components": "^2.4.29",
    "@kungfu-trader/kungfu-uicc": "^2.4.0-alpha.4",
    "electron-find": "^1.0.0",
    "electron-updater": "^4.4.0",
    "element-ui": "^2.15.0",
    "iconv-lite": "^0.6.0",
    "jschardet": "^2.3.0",
    "minimist": "^1.2.5",
    "monaco-editor": "^0.27.0",
    "monaco-editor-webpack-plugin": "^4.1.0",
    "vue": "^2.6.10",
    "vuex": "^3.6.0",
    "vue-router": "^3.5.0",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.10",
    "vue-virtual-scroller": "^1.0.10"
  },
  "devDependencies": {
    "@kungfu-trader/kungfu-toolchain": "^2.4.0-alpha.4",
    "webpack": "*"
  }
}
