{
  "name": "kfext_task_timer-trader",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "install-deps": "node -v",
    "dist": "cross-env NODE_ENV=production node --max_old_space_size=2048 ../node_modules/webpack/bin/webpack.js --progress --colors --config .build/webpack.ext.config.js",
    "build": "yarn run dist && yarn run cp",
    "build-cli": "yarn run dist && yarn run cp",
    "cp": "copyfiles -a -f ./package.json ./lib",
    "clean": "rimraf ./lib/*",
    "dev": "node .build/dev-ext.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "kungfu-shared": "*",
    "minimist": "^1.2.5",
    "moment": "^2.29.1",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.1",
    "@types/node": "^14.14.20",
    "copyfiles": "^2.4.1",
    "pkg": "^4.4.9",
    "rimraf": "^3.0.2"
  },
  "bin": "./lib/index.js",
  "kungfuConfig": {
    "type": "task",
    "subType": "trade",
    "key": "timerTrader",
    "name": "定时任务",
    "uniKey": [
      "accountId",
      "ticker"
    ],
    "config": [
      {
        "key": "accountId",
        "name": "账户",
        "type": "account",
        "required": true
      },
      {
        "key": "ticker",
        "name": "标的",
        "type": "instrumentId",
        "required": true
      },
      {
        "key": "exchangeId",
        "name": "交易所",
        "type": "exchangeId",
        "required": true
      },
      {
        "key": "side",
        "name": "买卖",
        "type": "radio",
        "data": [
          {
            "name": "买",
            "value": 0
          },
          {
            "name": "卖",
            "value": 1
          }
        ],
        "default": 0,
        "required": true
      },
      {
        "key": "offset",
        "name": "开平",
        "type": "radio",
        "data": [
          {
            "name": "开",
            "value": 0
          },
          {
            "name": "平",
            "value": 1
          }
        ],
        "default": 0,
        "required": true
      },
      {
        "key": "volume",
        "name": "数量",
        "type": "int",
        "required": true
      },
      {
        "key": "triggerTime",
        "name": "触发时间",
        "type": "timePicker",
        "required": true
      },
      {
        "key": "finishTime",
        "name": "结束时间",
        "type": "timePicker",
        "laterThan": "triggerTime",
        "required": true
      },
      {
        "key": "steps",
        "name": "总次数",
        "type": "int",
        "tip": "将以总次数均分总时长，建议保证每步间隔不能太短（>500ms）",
        "required": true
      },
      {
        "key": "maxLotByStep",
        "name": "单次最大手数",
        "type": "int",
        "tip": "各个交易所不同，默认100",
        "required": true,
        "default": 100
      },
      {
        "key": "lastSingularity",
        "name": "临界下单",
        "type": "bool",
        "required": false,
        "tip": "开启后最后一步下单操作会在结束时间前执行，默认1000毫秒",
        "default": true
      },
      {
        "key": "lastSingularityMilliSecond",
        "name": "临界下单时间",
        "type": "int",
        "required": false,
        "tip": "设置临界下单临界时间, 单位毫秒，时间默认1000毫秒",
        "default": 1000
      }
    ],
    "displayConfig": [
      {
        "type": "text",
        "label": "下单时间",
        "prop": "updateTime",
        "width": "80px"
      },{
        "type": "text",
        "label": "标的",
        "prop": "instrumentId",
        "width": "100px"
      },{
        "type": "number",
        "label": "最新价",
        "prop": "lastPrice",
        "width": "80px"
      },{
        "type": "number",
        "label": "卖1价/量",
        "prop": "ask1PriceVolume",
        "width": "150px"
      },{
        "type": "number",
        "label": "买1价/量",
        "prop": "bid1PriceVolume",
        "width": "150px"
      },{
        "type": "text",
        "label": "",
        "align": "center",
        "prop": "side",
        "width": "30px"
      },{
        "type": "text",
        "align": "center",
        "label": "",
        "prop": "offset",
        "width": "30px"
      },{
        "type": "number",
        "label": "委托价",
        "prop": "limitPrice",
        "width": "80px"
      },{
        "type": "number",
        "label": "下单量",
        "prop": "volume",
        "width": "60px"
      },{
        "type": "text",
        "align": "right",
        "label": "成交/目标量",
        "prop": "volumeLefted",
        "width": "100px"
      },{
        "type": "text",
        "align": "right",
        "label": "次数",
        "prop": "count",
        "width": "60px"
      },{
        "type": "text",
        "align": "left",
        "label": "账户",
        "prop": "accountId",
        "width": "110px"
      }
    ]
  }
}
