{
  "name": "kungfu",
  "author": "taurus.ai",
  "version": "2.3.0",
  "description": "KungFu Trader",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "shared",
    "core",
    "cli2",
    "kfext_task_timerTrader",
    "app",
    "tests"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run env-electron && yarn wsrun --serial --fast-exit build",
    "build-cli": "yarn run clean && yarn run env-cli && yarn wsrun --serial --fast-exit build-cli",
    "env-cli": "npm config set kungfu-core:runtime node",
    "env-electron": "npm config set kungfu-core:runtime electron",
    "env-pypi-python": "npm config set kungfu-core:pypi https://pypi.python.org/simple",
    "env-pypi-aliyun": "npm config set kungfu-core:pypi http://mirrors.aliyun.com/pypi/simple",
    "clean": "yarn wsrun clean"
  },
  "dependencies": {
    "wsrun": "^5.0.0",
    "cross-env": "1.0.0",
    "cross-var": "1.0.0",
    "nan": "0.3.0",
    "node-gyp": "^7.0.0",
    "node-pre-gyp": "^0.11.0"
  }
}
