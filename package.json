{
  "name": "kungfu",
  "author": "taurus.ai",
  "version": "2.3.0",
  "description": "KungFu Trader",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "shared",
    "core",
    "cli2",
    "kfext_task_timerTrader",
    "app",
    "tests"
  ],
  "scripts": {
    "build": "wsrun --serial --fast-exit --exclude-missing build",
    "build-cli": "wsrun --serial --fast-exit --exclude-missing build-cli",
    "env-pypi-python": "npm config set @kungfu-trader/kungfu-core:pypi https://pypi.python.org/simple",
    "env-pypi-aliyun": "npm config set @kungfu-trader/kungfu-core:pypi http://mirrors.aliyun.com/pypi/simple",
    "clean": "wsrun clean",
    "publish-package": "wsrun --serial --fast-exit --exclude-missing publish-package"
  },
  "dependencies": {
    "cross-env": "1.0.0",
    "cross-var": "1.0.0",
    "node-gyp": "^7.1.0",
    "node-pre-gyp": "^0.17.0",
    "wsrun": "^5.0.0"
  }
}
