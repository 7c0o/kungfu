{
  "name": "@kungfu-trader/kfx-vendor-sim",
  "author": {
    "name": "kungfu-trader",
    "email": "info@kungfu.link"
  },
  "version": "2.4.0-alpha.4",
  "description": "Kungfu Extension - SIM",
  "license": "Apache-2.0",
  "repository": {
    "url": "https://github.com/kungfu-trader/kungfu.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "configure": "kfs extension configure",
    "build": "kfs extension build",
    "dev": "kfs craft dev",
    "format": "kfc engage black src/python *.py"
  },
  "devDependencies": {
    "@kungfu-trader/kungfu-sdk": "^2.4.0-alpha.4"
  },
  "kungfuBuild": {
    "python": {
      "dependencies": {
        "dotted_dict": "~=1.1.0",
        "recordclass": "~=0.15.0",
        "sortedcontainers": "~=2.4.0"
      }
    }
  },
  "kungfuConfig": {
    "key": "sim",
    "name": "SIM",
    "config": {
      "td": {
        "type": ["stock", "future", "option"],
        "settings": [
          {
            "key": "account_id",
            "name": "account_id",
            "type": "str",
            "errMsg": "请填写账户 account_id",
            "required": true,
            "primary": true,
            "validator": [
              "noUnderlineValidator"
            ]
          },
          {
            "key": "match_mode",
            "name": "match_mode",
            "type": "select",
            "data": [
              {
                "value": "reject",
                "name": "reject"
              },
              {
                "value": "pend",
                "name": "pend"
              },
              {
                "value": "cancel",
                "name": "cancel"
              },
              {
                "value": "partialfillandcancel",
                "name": "partialfillandcancel"
              },
              {
                "value": "partialfill",
                "name": "partialfill"
              },
              {
                "value": "fill",
                "name": "fill"
              }
            ],
            "errMsg": "请选择撮合模式",
            "required": true
          }
        ]
      }
    }
  }
}