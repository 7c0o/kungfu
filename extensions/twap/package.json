{
  "name": "@kungfu-trader/kfx-twap",
  "author": {
    "name": "kungfu-trader",
    "email": "info@kungfu.link"
  },
  "version": "2.4.0-alpha.46",
  "description": "Kungfu Extension - TWAP",
  "license": "Apache-2.0",
  "main": "package.json",
  "repository": {
    "url": "https://github.com/kungfu-trader/kungfu.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "binary": {
    "module_name": "kfx-twap",
    "module_path": "dist/twap",
    "remote_path": "{module_name}/v{major}/v{version}",
    "package_name": "{module_name}-v{version}-{platform}-{arch}-{configuration}.tar.gz",
    "host": "https://prebuilt.libkungfu.cc"
  },
  "scripts": {
    "build": "kfs extension build",
    "clean": "kfs extension clean",
    "install": "node -e \"require('@kungfu-trader/kungfu-core').prebuilt('install')\"",
    "package": "kfs project package",
    "dev": "kfs craft dev"
  },
  "devDependencies": {
    "@kungfu-trader/kungfu-sdk": "^2.4.0-alpha.46"
  },
  "kungfuBuild": {
    "python": {
      "dependencies": {}
    }
  },
  "kungfuConfig": {
    "key": "twap",
    "name": "TWAP",
    "config": {
      "strategy": {
        "type": "trade",
        "settings": [
          {
            "key": "accountId",
            "name": "账户",
            "type": "td",
            "required": true,
            "primary": true
          },
          {
            "key": "marketSource",
            "name": "行情",
            "type": "md",
            "required": true,
            "primary": true
          },
          {
            "key": "ticker",
            "name": "标的",
            "type": "instrument",
            "required": true,
            "primary": false
          },
          {
            "key": "side",
            "name": "买卖",
            "type": "side",
            "default": 0,
            "required": true,
            "primary": true
          },
          {
            "key": "offset",
            "name": "开平",
            "type": "offset",
            "default": 0,
            "required": true,
            "primary": true
          },
          {
            "key": "priceMode",
            "name": "价格",
            "type": "select",
            "options": [
              {
                "label": "对手价一档",
                "value": "0"
              },
              {
                "label": "同方向一档",
                "value": "1"
              },
              {
                "label": "最新价",
                "value": "2"
              }
            ],
            "default": "0",
            "required": true
          },
          {
            "key": "priceType",
            "name": "下单方式",
            "type": "priceType",
            "default": "5",
            "required": true
          },
          {
            "key": "startTime",
            "name": "开始时间",
            "type": "timePicker",
            "tip": "开始时间若早于当前时间, 则视为立即启动",
            "required": true
          },
          {
            "key": "interval",
            "name": "下单间隔(ms)",
            "type": "int",
            "tip": "下单时间间隔, 建议间隔不能太短(>500ms)",
            "default": 1000,
            "required": true
          },
          {
            "key": "volume",
            "name": "每次下单数量",
            "type": "int",
            "required": true
          },
          {
            "key": "maxLotByStep",
            "name": "单次最大手数",
            "type": "int",
            "tip": "各个交易所不同, 默认100",
            "required": true,
            "default": 100
          },
          {
            "key": "info_a",
            "name": "info_a显示内容",
            "type": "text",
            "tip": "配置显示到前端的内容",
            "required": true,
            "default": "${info_a}"
          }
        ]
      }
    }
  }
}
