{
  "name": "@kungfu-trader/kungfu-app",
  "author": "Kungfu Origin",
  "version": "2.4.0-alpha.2",
  "description": "Kungfu App",
  "license": "Apache-2.0",
  "env": [
    "USE_HARD_LINKS=false"
  ],
  "main": "./dist/app/main.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "clean": "rimraf ./dist/* ./build/*",
    "dev": "node .electron-kungfu/dev-runner.js",
    "dist:main": "cross-env NODE_ENV=production node --max_old_space_size=2048 ../node_modules/webpack/bin/webpack.js --config .electron-kungfu/webpack.main.config.js",
    "dist:render": "cross-env NODE_ENV=production node --max_old_space_size=2048 ../node_modules/webpack/bin/webpack.js --config .electron-kungfu/webpack.renderer.config.js",
    "dist": "node --max_old_space_size=4096 .electron-kungfu/build.js",
    "elec": "node .electron-builder/build.js",
    "prebuild": "yarn run clean && yarn run dist",
    "build": "yarn run elec && yarn run elec --pro",
    "prebuild-cli": "yarn run clean && yarn run dist",
    "build-cli": "yarn run elec --cli"
  },
